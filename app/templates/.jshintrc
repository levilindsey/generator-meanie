{
  "node": true,
  "browser": true,
  "es5": true,
  "esnext": true,
  "bitwise": false,
  "camelcase": false,
  "curly": false,
  "eqeqeq": true,
  "immed": true,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "quotmark": "single",
  "regexp": true,
  "undef": true,
  "unused": true,
  "strict": false,
  "trailing": true,
  "smarttabs": false,
  "globals": {
     "angular": true
  },
  "predef": [
    "define",
    "require",
    "exports",
    "module",
    "describe",
    "before",
    "beforeEach",
    "after",
    "afterEach",
    "it"
  ],
  "indent": 2,
  "maxlen": 120,
  "devel": false
}
